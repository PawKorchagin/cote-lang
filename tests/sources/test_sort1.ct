fn sort(arr) {
    for (i = 0; i < len(arr); i += 1) {
        for (j = i + 1; j < len(arr); j += 1) {
            if (arr[i] > arr[j]) {
                t = arr[i];
                arr[i] = arr[j];
                arr[j] = t;
            }
        }
    }
}
fn main() {
    n = 50000;
    arr = array(n);

    for (i = 0; i < len(arr); i += 1) {
        arr[i] = rand();
    }
    sort(arr);
    cnt = 0;
    for (i = 0; i + 1 < len(arr); i += 1) {
        cnt += arr[i] <= arr[i + 1];
        println(arr[i]);
    }
    if (cnt - n + 1) throw();
    return cnt - n + 1;
}