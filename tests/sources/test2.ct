fn main() {
    p1 = 0;
    p2 = 1;
    n = 1;
    if (n == 0) n = 20;
    else if (n == 1) n = 10;
    else n = 20;
    if (n == 20)
        return 0;
    //print(n);
    for (i = 0; i < n; i += 1) {
        s = p1 + p2;
        p1 = p2;
        p2 = s;
    }
    return p1 - 55;
}
